  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
<% end %>

<%= stylesheet_link_tag    "application", media: "all" %>

<%= javascript_include_tag "application" %>
<%= javascript_include_tag "digital_objects_app" %>
<%= javascript_include_tag "/bootstrap-multiselect-0.9.2/js/bootstrap-multiselect" %>
<%= javascript_include_tag "/ace-1.1.8-min-noconflict/ace.js" %>
<%= javascript_include_tag "/ace-1.1.8-min-noconflict/ext-beautify.js" %>

<!-- OHSynchronizer -->
<%= javascript_include_tag "/synchronizer-module/js/ableplayer.min.js" %>
