<%= javascript_include_tag "/video-js-8.2.1/video.min.js" %>
<%= javascript_include_tag "/synchronizer-module/js/ohsynchronizer.js" %>
<%= javascript_include_tag "/synchronizer-module/js/js.cookie.2.2.0.min.js" %>

<%= csrf_meta_tags %>
