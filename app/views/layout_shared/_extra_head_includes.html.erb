<meta charset="utf-8" />
<link rel="shortcut icon" href="<%= image_path('favicon.png') %>" type="image/png" />

<%= stylesheet_link_tag "/bootstrap-multiselect-0.9.2/css/bootstrap-multiselect" %>

<!-- OHSynchronizer -->
<%= stylesheet_link_tag "/synchronizer-module/css/ableplayer.min.css" %>
<%= stylesheet_link_tag "/synchronizer-module/css/style.css" %>

<!-- Video player for non-synchronizer scenarios -->
<%= stylesheet_link_tag "/video-js-8.2.1/video-js.min.css" %>

<% unless Rails.env.test? %>
